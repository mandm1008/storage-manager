generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model GoogleAccount {
  id           Int      @id @default(autoincrement())
  email        String
  accessToken  String
  refreshToken String?
  expiryDate   BigInt?
  createdAt    DateTime @default(now())
}
